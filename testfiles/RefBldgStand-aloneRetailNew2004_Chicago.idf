    0.52357,                 !- Coefficient1 Constant
    0.03478,                 !- Coefficient2 x
    0,                       !- Coefficient3 x**2
    -0.001915,               !- Coefficient4 y
    -0.00010838,             !- Coefficient5 y**2
    0,                       !- Coefficient6 x*y
    11.1,                    !- Minimum Value of x
    29.4,                    !- Maximum Value of x
    10.0,                    !- Minimum Value of y
    50.3;                    !- Maximum Value of y

