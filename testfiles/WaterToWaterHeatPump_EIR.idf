! WaterToWaterHeatPump_EIR.idf
!
! Basic file description:
! The EIR formulated water-to-water heat pump is demonstrated using a simple collection of plant loops.  Building loads
!  are represented using PLant Load Profile objects
! Run:      1 design day.
! Building: None.
! System:   None.
! Plant:    PLANT LOAD PROFILE with District Heating and Cooling as necessary.

  Version,8.9;

  Building,
    Plant Load Profile Example,  !- Name
    0.0,                     !- North Axis {deg}
    Suburbs,                 !- Terrain
    0.04,                    !- Loads Convergence Tolerance Value
    0.04,                    !- Temperature Convergence Tolerance Value {deltaC}
    FullInteriorAndExterior, !- Solar Distribution
    25,                      !- Maximum Number of Warmup Days
    6;                       !- Minimum Number of Warmup Days

  Timestep,1;

  GlobalGeometryRules,
    UpperLeftCorner,         !- Starting Vertex Position
    CounterClockWise,        !- Vertex Entry Direction
    Relative;                !- Coordinate System

  Site:Location,
    Chicago,                 !- Name
    42.00,                   !- Latitude {deg}
    -87.88,                  !- Longitude {deg}
    -6.00,                   !- Time Zone {hr}
    190.00;                  !- Elevation {m}

  SizingPeriod:DesignDay,
    Chicago Summer 1%,       !- Name
    7,                       !- Month
    21,                      !- Day of Month
    SummerDesignDay,         !- Day Type
    31.5,                    !- Maximum Dry-Bulb Temperature {C}
    10.7,                    !- Daily Dry-Bulb Temperature Range {deltaC}
    ,                        !- Dry-Bulb Temperature Range Modifier Type
    ,                        !- Dry-Bulb Temperature Range Modifier Day Schedule Name
    Wetbulb,                 !- Humidity Condition Type
    23,                      !- Wetbulb or DewPoint at Maximum Dry-Bulb {C}
    ,                        !- Humidity Condition Day Schedule Name
    ,                        !- Humidity Ratio at Maximum Dry-Bulb {kgWater/kgDryAir}
    ,                        !- Enthalpy at Maximum Dry-Bulb {J/kg}
    ,                        !- Daily Wet-Bulb Temperature Range {deltaC}
    99063.,                  !- Barometric Pressure {Pa}
    5.3,                     !- Wind Speed {m/s}
    230,                     !- Wind Direction {deg}
    No,                      !- Rain Indicator
    No,                      !- Snow Indicator
    No,                      !- Daylight Saving Time Indicator
    ASHRAEClearSky,          !- Solar Model Indicator
    ,                        !- Beam Solar Day Schedule Name
    ,                        !- Diffuse Solar Day Schedule Name
    ,                        !- ASHRAE Clear Sky Optical Depth for Beam Irradiance (taub) {dimensionless}
    ,                        !- ASHRAE Clear Sky Optical Depth for Diffuse Irradiance (taud) {dimensionless}
    1.00;                    !- Sky Clearness

  SimulationControl,
    No,                      !- Do Zone Sizing Calculation
    No,                      !- Do System Sizing Calculation
    No,                      !- Do Plant Sizing Calculation
    Yes,                     !- Run Simulation for Sizing Periods
    No;                      !- Run Simulation for Weather File Run Periods

  ScheduleTypeLimits,
    Any Number;              !- Name

  ScheduleTypeLimits,
    On/Off,                  !- Name
    0,                       !- Lower Limit Value
    1,                       !- Upper Limit Value
    Discrete;                !- Numeric Type

  Schedule:Constant,
    Plant Loop Temp Sch,     !- Name
    Any Number,              !- Schedule Type Limits Name
    60.0;                    !- Value
    
  Schedule:Constant,
    Condenser Loop Temp Sch, !- Name
    Any Number,              !- Schedule Type Limits Name
    60.0;                    !- Value
    
  Schedule:Constant,
    AlwaysOnSchedule,        !- Name
    On/Off,                  !- Schedule Type Limits Name
    1;                       !- Value

  Schedule:Compact,
    Load Profile 1 Load Schedule,  !- Name
    Any Number,              !- Schedule Type Limits Name
    THROUGH: 12/31,          !- Field 1
    FOR: AllDays,            !- Field 2
    UNTIL: 4:00,8000,        !- Field 3
    UNTIL: 8:00,6000,        !- Field 5
    UNTIL: 9:00,0,           !- Field 7
    UNTIL: 12:00,6000,       !- Field 9
    UNTIL: 24:00,10000;      !- Field 11
    
  Schedule:Constant,
    Load Profile 1 Flow Frac Schedule,  !- Name
    Any Number,              !- Schedule Type Limits Name
    1.0;                     !- Value

  PlantLoop,
    Plant Loop,              !- Name
    WATER,                   !- Fluid Type
    ,                        !- User Defined Fluid Type
    Plant Loop Operation,    !- Plant Equipment Operation Scheme Name
    Plant Supply Outlet Node,  !- Loop Temperature Setpoint Node Name
    100,                     !- Maximum Loop Temperature {C}
    3,                       !- Minimum Loop Temperature {C}
    0.003,                   !- Maximum Loop Flow Rate {m3/s}
    0,                       !- Minimum Loop Flow Rate {m3/s}
    1,                       !- Plant Loop Volume {m3}
    Plant Supply Inlet Node,  !- Plant Side Inlet Node Name
    Plant Supply Outlet Node,  !- Plant Side Outlet Node Name
    Plant Supply Branches,   !- Plant Side Branch List Name
    ,                        !- Plant Side Connector List Name
    Plant Demand Inlet Node,  !- Demand Side Inlet Node Name
    Plant Demand Outlet Node,  !- Demand Side Outlet Node Name
    Plant Demand Branches,   !- Demand Side Branch List Name
    ,                        !- Demand Side Connector List Name
    Optimal;                 !- Load Distribution Scheme

  SetpointManager:Scheduled,
    Plant Loop Setpoint Manager,  !- Name
    Temperature,             !- Control Variable
    Plant Loop Temp Sch,     !- Schedule Name
    Plant Supply Outlet Node;  !- Setpoint Node or NodeList Name

  PlantEquipmentOperationSchemes,
    Plant Loop Operation,    !- Name
    PlantEquipmentOperation:HeatingLoad,  !- Control Scheme 1 Object Type
    Plant Heating Operation,  !- Control Scheme 1 Name
    AlwaysOnSchedule;        !- Control Scheme 1 Schedule Name

  PlantEquipmentOperation:HeatingLoad,
    Plant Heating Operation,  !- Name
    0,                       !- Load Range 1 Lower Limit {W}
    10000000,                !- Load Range 1 Upper Limit {W}
    Plant Heating Equipment;  !- Range 1 Equipment List Name

  PlantEquipmentList,
    Plant Heating Equipment,  !- Name
    HeatPump:WaterToWater:EIR:Heating,  !- Equipment 1 Object Type
    Plant Heat Pump;         !- Equipment 1 Name

  BranchList,
    Plant Supply Branches,   !- Name
    Plant Supply Branch;     !- Branch 1 Name

  Branch,
    Plant Supply Branch,     !- Name
    ,                        !- Pressure Drop Curve Name
    Pump:VariableSpeed,      !- Component 1 Object Type
    Plant Pump,              !- Component 1 Name
    Plant Supply Inlet Node,  !- Component 1 Inlet Node Name
    Plant Supply Intermediate Node,  !- Component 1 Outlet Node Name
    HeatPump:WaterToWater:EIR:Heating,  !- Component 1 Object Type
    Plant Heat Pump,         !- Component 1 Name
    Plant Supply Intermediate Node,  !- Component 1 Inlet Node Name
    Plant Supply Outlet Node;  !- Component 1 Outlet Node Name

  Pump:VariableSpeed,
    Plant Pump,              !- Name
    Plant Supply Inlet Node,  !- Inlet Node Name
    Plant Supply Intermediate Node,!- Outlet Node Name
    0.005,                   !- Design Maximum Flow Rate {m3/s}
    300000,                  !- Design Pump Head {Pa}
    2250,                    !- Design Power Consumption {W}
    0.87,                    !- Motor Efficiency
    0.0,                     !- Fraction of Motor Inefficiencies to Fluid Stream
    0,                       !- Coefficient 1 of the Part Load Performance Curve
    1,                       !- Coefficient 2 of the Part Load Performance Curve
    0,                       !- Coefficient 3 of the Part Load Performance Curve
    0,                       !- Coefficient 4 of the Part Load Performance Curve
    0,                       !- Design Minimum Flow Rate {m3/s}
    INTERMITTENT;            !- Pump Control Type

  HeatPump:WaterToWater:EIR:Heating,
    Plant Heat Pump,         !- Name
    Plant Supply Intermediate Node, !- Load Side Inlet Node Name
    Plant Supply Outlet Node,  !- Load Side Outlet Node Name
    Condenser Demand Inlet Node,  !- Source Side Inlet Node Name
    Condenser Demand Outlet Node,  !- Source Side Outlet Node Name
    0.003,                   !- Load Side Design Volume Flow Rate {m3/s}
    0.003;                   !- Source Side Design Volume Flow Rate {m3/s}

  BranchList,
    Plant Demand Branches,   !- Name
    Plant Demand Branch;     !- Branch 1 Name

  Branch,
    Plant Demand Branch,     !- Name
    ,                        !- Pressure Drop Curve Name
    LoadProfile:Plant,       !- Component 1 Object Type
    Load Profile 1,          !- Component 1 Name
    Plant Demand Inlet Node,  !- Component 1 Inlet Node Name
    Plant Demand Outlet Node;  !- Component 1 Outlet Node Name

  LoadProfile:Plant,
    Load Profile 1,          !- Name
    Plant Demand Inlet Node,  !- Inlet Node Name
    Plant Demand Outlet Node,  !- Outlet Node Name
    Load Profile 1 Load Schedule,  !- Load Schedule Name
    0.002,                   !- Peak Flow Rate {m3/s}
    Load Profile 1 Flow Frac Schedule;  !- Flow Rate Fraction Schedule Name

  PlantLoop,
    Condenser Loop,          !- Name
    WATER,                   !- Fluid Type
    ,                        !- User Defined Fluid Type
    Condenser Loop Operation,  !- Plant Equipment Operation Scheme Name
    Condenser Supply Outlet Node,  !- Loop Temperature Setpoint Node Name
    100,                     !- Maximum Loop Temperature {C}
    3,                       !- Minimum Loop Temperature {C}
    0.003,                   !- Maximum Loop Flow Rate {m3/s}
    0,                       !- Minimum Loop Flow Rate {m3/s}
    1,                       !- Plant Loop Volume {m3}
    Condenser Supply Inlet Node,  !- Plant Side Inlet Node Name
    Condenser Supply Outlet Node,  !- Plant Side Outlet Node Name
    Condenser Supply Branches,  !- Plant Side Branch List Name
    ,                        !- Plant Side Connector List Name
    Condenser Demand Inlet Node,  !- Demand Side Inlet Node Name
    Condenser Demand Outlet Node,  !- Demand Side Outlet Node Name
    Condenser Demand Branches,    !- Demand Side Branch List Name
    ,                        !- Demand Side Connector List Name
    Optimal;                 !- Load Distribution Scheme

  SetpointManager:Scheduled,
    Condenser Loop Setpoint Manager,  !- Name
    Temperature,             !- Control Variable
    Condenser Loop Temp Sch,  !- Schedule Name
    Condenser Supply Outlet Node;  !- Setpoint Node or NodeList Name

  PlantEquipmentOperationSchemes,
    Condenser Loop Operation,  !- Name
    PlantEquipmentOperation:HeatingLoad,  !- Control Scheme 1 Object Type
    Condenser Heating Operation,  !- Control Scheme 1 Name
    AlwaysOnSchedule;        !- Control Scheme 1 Schedule Name

  PlantEquipmentOperation:HeatingLoad,
    Condenser Heating Operation,  !- Name
    0,                       !- Load Range 1 Lower Limit {W}
    1000000000,              !- Load Range 1 Upper Limit {W}
    Condenser Heating Equipment;  !- Range 1 Equipment List Name

  PlantEquipmentList,
    Condenser Heating Equipment,  !- Name
    DistrictHeating,         !- Equipment 1 Object Type
    Condenser Purchased Heating;  !- Equipment 1 Name

  BranchList,
    Condenser Supply Branches,  !- Name
    Condenser Supply Branch;  !- Branch 1 Name

  Branch,
    Condenser Supply Branch, !- Name
    ,                        !- Pressure Drop Curve Name
    Pump:VariableSpeed,      !- Component 1 Object Type
    Condenser Pump,          !- Component 1 Name
    Condenser Supply Inlet Node,  !- Component 1 Inlet Node Name
    Condenser Supply Intermediate Node,!- Component 1 Outlet Node Name
    DistrictHeating,         !- Component 1 Object Type
    Condenser Purchased Heating,  !- Component 1 Name
    Condenser Supply Intermediate Node,  !- Component 1 Inlet Node Name
    Condenser Supply Outlet Node;  !- Component 1 Outlet Node Name

  Pump:VariableSpeed,
    Condenser Pump,          !- Name
    Condenser Supply Inlet Node,  !- Inlet Node Name
    Condenser Supply Intermediate Node,  !- Outlet Node Name
    0.005,                   !- Design Maximum Flow Rate {m3/s}
    30000,                   !- Design Pump Head {Pa}
    225,                     !- Design Power Consumption {W}
    0.87,                    !- Motor Efficiency
    0.0,                     !- Fraction of Motor Inefficiencies to Fluid Stream
    0,                       !- Coefficient 1 of the Part Load Performance Curve
    1,                       !- Coefficient 2 of the Part Load Performance Curve
    0,                       !- Coefficient 3 of the Part Load Performance Curve
    0,                       !- Coefficient 4 of the Part Load Performance Curve
    0,                       !- Design Minimum Flow Rate {m3/s}
    Intermittent;            !- Pump Control Type

  DistrictHeating,
    Condenser Purchased Heating,  !- Name
    Condenser Supply Intermediate Node,  !- Hot Water Inlet Node Name
    Condenser Supply Outlet Node,  !- Hot Water Outlet Node Name
    100000000;               !- Nominal Capacity {W}

  BranchList,
    Condenser Demand Branches,  !- Name
    Condenser Demand Branch;  !- Branch 1 Name

  Branch,
    Condenser Demand Branch,  !- Name
    ,                        !- Pressure Drop Curve Name
    HeatPump:WaterToWater:EIR:Heating,  !- Component 1 Object Type
    Plant Heat Pump,         !- Component 1 Name
    Condenser Demand Inlet Node,  !- Component 1 Inlet Node Name
    Condenser Demand Outlet Node;  !- Component 1 Outlet Node Name

  Output:VariableDictionary,IDF;
  Output:Diagnostics,ReportDuringWarmup;
  Output:Variable,*,System Node Temperature,Hourly;
  Output:Variable,*,Plant Load Profile Mass Flow Rate,Hourly;
  Output:Variable,*,Plant Load Profile Heat Transfer Rate,Hourly;
  Output:Variable,*,District Heating Hot Water Rate,Hourly;
  Output:Variable,*,EIR WWHP Load Side Heat Transfer,hourly; !- HVAC Average [W]
  Output:Variable,*,EIR WWHP Source Side Heat Transfer,hourly; !- HVAC Average [W]
  Output:Variable,*,EIR WWHP Load Side Inlet Temperature,hourly; !- HVAC Average [C]
  Output:Variable,*,EIR WWHP Load Side Outlet Temperature,hourly; !- HVAC Average [C]
  Output:Variable,*,EIR WWHP Source Side Inlet Temperature,hourly; !- HVAC Average [C]
  Output:Variable,*,EIR WWHP Source Side Outlet Temperature,hourly; !- HVAC Average [C]
  Output:Variable,*,EIR WWHP Power Usage,hourly; !- HVAC Average [W]
